interface Controllable{
	void play();
	void stop();
}

class VideoPlayerTest {
	

	public static void main(String[] args) {
		Controllable VideoPlayer = new Controllable() {
			public void play() {
				System.out.println("play");
			}
			public void stop() {
				System.out.println("stop");
			}
		};
		VideoPlayer.play();
		VideoPlayer.stop();
		
		
		
	}

}
