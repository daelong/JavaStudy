import java.util.*;

class Charac{
	String userInput;
	
	private void cha(String getUserInput) {
		for(int i = 0; i < getUserInput.length(); i++)
			System.out.print(getUserInput.concat(getUserInput));
		System.out.println();
	}
	
	public String getUserInput(String prompt) {
		System.out.println(prompt);
		Scanner s = new Scanner(System.in);
		return s.next();
		
	}
	
	public void StartCharac() {
		userInput = getUserInput("문자를 입력하세요 : ");
		cha();
		
	}
}


public class Problem5 {

	public static void main(String[] args) {
		Charac ch = new Charac();
	}

}
