
public class BigIntegerTest {

	public static void main(String[] args) {
		String a = "12";
		String b = "12";
		String result = "";
		for(int i = 1; i < 2; i++) {
			result += Integer.parseInt(a.substring(0,1)) + Integer.parseInt(b.substring(0,1));
			if(Integer.parseInt(a.substring(0,i)) + Integer.parseInt(b.substring(0,i)) > 10) {
				result += Integer.parseInt(a.substring(0,1));
				result += Integer.parseInt(a.substring(i, i+1))+Integer.parseInt(b.substring(i, i+1))+ 1; 
			}
		//	result += Integer.parseInt(a.substring(0,i));
			
		}
		//result += Integer.parseInt(a.substring(0,1));
		System.out.println(result);
	}

}
