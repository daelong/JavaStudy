import java.util.*;

public class Numeral {

	public static void main(String[] args) {
		int n = 9;
		int x = 142;
		int y = 0;
		int i = 1;
		int result = 0;
		/*System.out.println(y);
		x = x/n;
		y = x%n;
		x = x/n;
		System.out.println(y);
		System.out.println(x);
		x = x*100;
		*/
		while(x>n) {
			y = x/n; //15
			x = x%n; //7
			x = x*i; //7
			i = i*10;
			System.out.println(x);
			result += x;
			x = y;
		}
		y = y*i;
		System.out.println(y);
		result = result + y;
		System.out.println(result);
	}

}
