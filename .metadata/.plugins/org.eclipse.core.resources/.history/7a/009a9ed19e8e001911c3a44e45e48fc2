import java.util.*;

class reverse{
	public reverse(String a) {
		for(int i = a.length()-1; i > 0; i--) {
			System.out.print(a.charAt(i));
		}
		System.out.println();
	}
}
class reverseString {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		String a = scanner.next();
		new reverse(a);

	}

}
