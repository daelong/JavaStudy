
public class BigIntegerTest {

	public static void main(String[] args) {
		String a = "92";
		String b = "92";
		String result = "";
		for(int i = 2; i < 3 ; i++) {
			//result += Integer.parseInt(a.substring(0,1)) + Integer.parseInt(b.substring(0,1));
			if(Integer.parseInt(a.substring(i-1,i)) + Integer.parseInt(b.substring(i-1,i)) > 10) {
				result += Integer.parseInt(a.substring(i-1, i))+Integer.parseInt(b.substring(i-1, i))+ 1;
				System.out.println(Integer.parseInt(a.substring(i-1, i)));
			}
		//	result += Integer.parseInt(a.substring(0,i));
			
		}
		System.out.println(a.substring(1,2));
		System.out.println(result);
	}
}
